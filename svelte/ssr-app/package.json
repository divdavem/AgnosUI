{
	"name": "ssr-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "node server",
		"build": "wireit",
		"preview": "NODE_ENV=production node server",
		"check": "svelte-check --tsconfig ./tsconfig.json"
	},
	"dependencies": {
		"express": "^4.18.3",
		"compression": "^1.7.4",
		"sirv": "^2.0.4"
	},
	"devDependencies": {
		"@agnos-ui/style-bootstrap": "",
		"@agnos-ui/svelte": "",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"bootstrap": "^5.3.3",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6"
	},
	"wireit": {
		"build": {
			"command": "vite build --ssrManifest --outDir dist/client && vite build --ssr src/entry-server.ts --outDir dist/server",
			"dependencies": [
				"../lib:build"
			],
			"files": [
				"src/**",
				"index.html",
				"vite.config.ts",
				"svelte.config.js",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/**"
			]
		}
	}
}
